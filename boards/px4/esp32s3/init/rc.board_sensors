#!/bin/sh
#
# PX4 FMUv4 specific board sensors init
#------------------------------------------------------------------------------


icm42688p -s -R 2 start

if param compare -s SYS_HAS_MAG 1
then
	ist8310 start -I -a 12
fi

if param compare -s SYS_HAS_BARO 1
then
	spl06 -I -a 119 start
fi

flow3953 start -d /dev/ttyS2

#vl53l0x start -I
